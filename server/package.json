{
   "name": "server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "scripts": {
      "start": "ts-node dist/index.js",
      "build": "tsc",
      "dev2": "ts-node src/index.ts",
      "dev": "tsc-watch --onSuccess \"node dist/index.js\" ",
      "generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n peru",
      "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/cors": "^2.8.10",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.11",
      "@types/jsonwebtoken": "^8.5.1",
      "@types/morgan": "^1.9.2",
      "@types/node": "^14.14.41",
      "ts-node": "^9.1.1",
      "tsc-watch": "^4.2.9",
      "typescript": "^4.2.4"
   },
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "class-validator": "^0.13.1",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.10.0",
      "mysql": "^2.14.1",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.32"
   }
}
